{
  "name": "life-data",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "node out/test/main.js",
    "cli": "node out/main/cli.js",
    "release:test": "yarn shadow-cljs release test",
    "release:cli": "yarn shadow-cljs release cli",
    "watch:test:shadow": "yarn shadow-cljs watch test",
    "watch:test:node": "watch 'yarn run test' ./out/test",
    "watch:test": "yarn run-p watch:test:*",
    "watch:cli:shadow": "doppler run --mount .env -- yarn shadow-cljs watch cli"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "shadow-cljs": "2.26.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "watch": "^1.0.2"
  }
}
